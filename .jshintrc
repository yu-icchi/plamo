{
  "maxerr": 50,

  // Enforcing
  "bitwise": false,
  "camelcase": false,
  "curly": true,
  "eqeqeq": true,
  "forin": false,
  "immed": false,
  "indent": 2,
  "latedef": "nofunc",
  "newcap": true,
  "noarg": true,
  "noempty": false,
  "nonew": true,
  "plusplus": false,
  "quotmark": false,
  "undef": true,
  "unused": "vars",
  "strict": false,
  "trailing": true,
  "maxparams": 5,
  "maxdepth": 6,
  "maxstatements": 20,
  "maxcomplexity": 10,
  "maxlen": 100,

  // Relaxing
  "asi": false,
  "boss": false,
  "debug": false,
  "eqnull": false,
  "es5": false,
  "esnext": true,
  "moz": false,
  "evil": false,
  "expr": true,
  "funcscope": false,
  "globalstrict": false,
  "iterator": false,
  "lastsemic": false,
  "laxbreak": false,
  "laxcomma": true,
  "loopfunc": true,
  "multistr": false,
  "proto": true,
  "scripturl": false,
  "smarttabs": true,
  "shadow": false,
  "sub": false,
  "supernew": true,
  "validthis": false,

  // Environments
  "browser": false,
  "couch": false,
  "devel": false,
  "dojo": false,
  "jquery": false,
  "mootools": false,
  "node": true,
  "nonstandard": true,
  "prototypejs": false,
  "rhino": false,
  "worker": false,
  "wsh": false,
  "yui": false,

  // Legacy
  "nomen": false,
  "onevar": false,
  "passfail": false,
  "white": false,

  // Custom Globals
  "globals": {
    "describe": false,
    "it": false,
    "before": false,
    "beforeEach": false,
    "after": false,
    "afterEach": false
  }
}
